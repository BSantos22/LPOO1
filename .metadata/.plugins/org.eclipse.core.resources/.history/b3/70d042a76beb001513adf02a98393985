package gametest;

import org.junit.Test;
import gamelogic.*;

public class TestMazeBuilder {

	@Test
	public void printMaze() {
		char[][] maze = MazeBuilder.buildMaze(10);
		
		for (int i = 0; i < 10; i++) {
			for (int j = 0; j < 10; j++) {
				System.out.print(maze[i][j]);
			}
			
			System.out.println();
		}
	}
}
