
public class Node {
	public Node parent;
	public String name;
	public Node[] child;
	
	public Node (Node parent, String name) {
		this.parent = parent;
		this.name = name;
	}
	
	public Folder getParent() {
		return parent;
	}
	
	public String getName() {
		return name;
	}
	
	public Node[] getChild() {
		return child;
	}

}
