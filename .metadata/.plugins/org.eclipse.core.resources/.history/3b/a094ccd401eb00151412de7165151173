package statistics;

public class NormalDistribution {
	private double mean;
	private double stddev;
	
	public NormalDistribution() {
		this.mean = 0;
		this.stddev = 0;
	}
	
	public NormalDistribution(double mean, double stddev) {
		this.mean = mean;
		this.stddev = stddev;
	}

	public double getMean() {
		return mean;
	}

	public double getStddev() {
		return stddev;
	}
}
